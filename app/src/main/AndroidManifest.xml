            android:name="com.example.administrator.myapplication.ThreadPoolTest.ThreadPoolActivty"
            android:screenOrientation="portrait" />

        <activity
            android:name=".GridViewTest.GridViewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ArchitectureComponents.Paging.PagingActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".AIDLTest.BookManagerClient"
            android:screenOrientation="portrait" >
        </activity>

        <activity android:name=".RecyclerViewDragTest.RecyclerViewDragActivity"
            android:screenOrientation="portrait" >
        </activity>

        <activity android:name=".HandlerTest.HandlerTestActivity"
            android:screenOrientation="portrait" >
        </activity>

        <activity android:name=".Components.SpinnerActivity"
            android:screenOrientation="portrait" >
        </activity>

        <service android:name=".AIDLTest.BookManagerService"
            android:process=":remote"
            >

        </service>
    </application>

</manifest>